{
  "name": "angular.io-example",
  "version": "0.0.0",
  "description": "Example project from an angular.io guide.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0-rc.0",
    "@angular/common": "^16.1.0-rc.0",
    "@angular/compiler": "^16.1.0-rc.0",
    "@angular/core": "^16.1.0-rc.0",
    "@angular/forms": "^16.1.0-rc.0",
    "@angular/platform-browser": "^16.1.0-rc.0",
    "@angular/platform-browser-dynamic": "^16.1.0-rc.0",
    "@angular/router": "^16.1.0-rc.0",
    "@types/jest": "^29.5.14",
    "angular-in-memory-web-api": "~0.16.0",
    "bootstrap": "^5.3.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.1",
    "ngx-loading": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0-rc.0",
    "@angular/cli": "^16.1.0-rc.0",
    "@angular/compiler-cli": "^16.1.0-rc.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.35",
    "copyfiles": "^2.4.1",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~4.9.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
